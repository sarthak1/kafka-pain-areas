package com.example.kafka_pain_areas;

import org.springframework.kafka.annotation.KafkaListener;
import org.springframework.stereotype.Service;

@Service
public class HistoricalDataKafkaListener {

    @KafkaListener(topics = "${spring.kafka.consumer.topic.historical}", groupId = "${spring.kafka.consumer.group-id.historical}")
    public void listenHistoricalData(String message) {
        // Process historical data
        System.out.println("Received historical data: " + message);
        // ... existing code ...
    }
}